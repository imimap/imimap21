{
  "name": "server-template",
  "version": "1.0.0",
  "description": "Express.js template",
  "main": "src/server.ts",
  "scripts": {
    "serve": "node dist/server.js",
    "serve-debug": "node --inspect=0.0.0.0 dist/server.js",
    "build-ts": "tsc",
    "watch": "tsc-watch --onSuccess \"npm run serve-debug\"",
    "format": "prettier src/server.ts **/*.js --write",
    "lint": "eslint \"src/**\"",
    "test-mongoose": "jest --runInBand --detectOpenHandles ./src/models/test"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-async-handler": "^1.1.4",
    "express-jwt": "^6.0.0",
    "express-validator": "^6.12.0",
    "http-errors": "^1.7.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.4",
    "passport": "^0.4.1",
    "passport-ldapauth": "^3.0.1",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-flow": "^7.12.13",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-jwt": "^6.0.1",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.41",
    "@types/passport": "^1.0.6",
    "@types/validator": "^13.1.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^27.0.2",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.3",
    "mongod": "^2.0.0",
    "mongodb-memory-server": "^6.9.6",
    "nodemon": "^1.18.3",
    "prettier": "^2.0.5",
    "ts-jest": "^27.0.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.4"
  }
}
