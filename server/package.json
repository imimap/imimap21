{
  "name": "server-template",
  "version": "1.0.0",
  "description": "Express.js template",
  "main": "src/server.ts",
  "scripts": {
    "serve": "node dist/server.js",
    "build-ts": "tsc",
    "watch": "tsc-watch --onSuccess \"npm run serve\"",
    "format": "prettier src/server.ts **/*.js --write",
    "lint": "eslint \"src/**\"",
    "test-mongoose": "jest --runInBand --detectOpenHandles ./src/models/test"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]s?$": "babel-jest"
    }
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "mongoose": "^5.12.4"
  },
  "devDependencies": {
    "@babel/plugin-syntax-flow": "^7.12.13",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^27.0.2",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.3",
    "mongod": "^2.0.0",
    "mongodb-memory-server": "^6.9.6",
    "nodemon": "^1.18.3",
    "prettier": "^2.0.5",
    "ts-jest": "^27.0.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.4"
  }
}
